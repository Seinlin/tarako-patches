index 2e6612e..0000000
--- a/apps/settings/style/lists.css
+++ /dev/null
@@ -1,378 +0,0 @@
-/**
- * Table View
- * line height = 6rem, border included
- */
-
-ul {
-  margin: 0;
-  padding: 0;
-  overflow: hidden;
-  list-style: none;
-}
-
-ul li {
-  -moz-box-sizing: border-box;
-  position: relative;
-  min-height: 6rem;
-  margin: 0 1.5rem;
-  border-bottom: 0.1rem solid #e6e6e3;
-  color: #000;
-}
-
-ul li:last-child {
-  border-bottom: none;
-}
-
-ul li.sound-setting {
-  min-height: 7.9rem;
-}
-
-ul li label:not([for]) {
-  background: none;
-}
-
-ul li > a,
-ul li > span,
-ul li > label > span {
-  display: block;
-  text-decoration: none;
-  outline: 0;
-  color: #000;
-  -moz-box-sizing: border-box;
-  height: 6rem;
-  font-size: 1.9rem;
-  line-height: 6rem;
-  padding: 0 3rem;
-  margin: 0 -1.5rem;
-}
-
-ul li.active a,
-ul li.active span,
-ul li:not([aria-disabled="true"]) > small + a:active,
-ul li:not([aria-disabled="true"]) > a[href]:active,
-ul li:not([aria-disabled="true"]) > small + a:focus,
-ul li:not([aria-disabled="true"]) > a[href]:focus {
-  background-color: #b2f2ff;
-  color: #222;
-}
-
-ul li[aria-disabled="true"] > a {
-  color: #797e80;
-  opacity: 0.6;
-  pointer-events: none;
-}
-
-ul > li > progress {
-  display: inline-block;
-  margin: 0;
-  padding: 0;
-}
-
-ul > li > progress + span {
-  display: inline-block;
-  vertical-align: middle;
-  margin: 0;
-  padding: 0 0 0 1rem;
-  color: #505b5b;
-  font-size: 1.4rem;
-}
-
-/* description + value on the same line */
-ul li > a span {
-  position: absolute;
-  right: 1.5rem;
-  color: #505859;
-}
-
-/* text ellipsis */
-ul li > *, ul li > label > * {
-  display: block;
-  overflow: hidden;
-  text-overflow: ellipsis;
-  white-space: nowrap;
-}
-
